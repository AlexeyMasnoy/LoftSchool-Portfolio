  .login-box
    a.login#showlogin(href="") Авторизоваться
  .flip-container.welcome__card
    .flipper
      .selfbox.card
        .selfbox-head
          .selfbox-head-img-container
            img.selfbox-head-img(src="../../assets/img/myphoto.jpg", alt="")
          .selfbox-head-text
            h1.selfbox-h1 Алексей Масной
            h4.selfbox-p Личный сайт веб разработчика
        .selfbox-body
           ul.socials
            li.social-item
              a.social-link(href="")
                .social-icon
                  +use("vk","svg-vk-dims")
            li.social-item
              a.social-link(href="")
                .social-icon
                   +use("github","svg-github-dims")
            li.social-item
              a.social-link(href="")
                .social-icon
                  +use("in","svg-in-dims")
              

        +nav('selfbox-nav', {'Мои работы':'work.html', 'Обо мне':'about.html','Блог':'blog.html'})
      .box.card
        .selfbox-login
          h1.selfbox-login-text Авторизуйтесь
          form.login-form-man
            input(type="text", placeholder="Логин").login-input-log
            input(type="text", placeholder="Пароль").login-input-pas
          form.login-form-human
            input.login-input-chek(type="checkbox")
            span.span-text Я человек
          h5.login-form-text Вы точно не робот?
          form.login-input-not
            input.login-input-yes(type="radio" value="yes")
            span.yes-text Да
            input.login-input-no(type="radio" value="no")
            span.no-text Не уверен

        +nav('selfbox-nav', {'На главную':'index.html', 'Войти':'log-in.html'})
